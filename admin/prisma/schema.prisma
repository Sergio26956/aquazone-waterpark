generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Usuario {
  id       Int    @id @default(autoincrement())
  nombre   String
  correo   String @unique
  tipo     String
  estado   String
  creadoEn DateTime @default(now())
}
